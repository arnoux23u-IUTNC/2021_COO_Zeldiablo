}

Jeu "1" -down-> "1" Labyrinthe: " - labyrinthe"
Labyrinthe "1" -down-> "1" Entree: " - entree "
Labyrinthe "1" -right-> "*" Case: " - cases "
Jeu "1" -right-> "1" Joueur: " - joueur "

